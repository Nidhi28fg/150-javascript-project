<script>
// Get a reference to the table body element
const tableBody = document.getElementById('ordersManagmentTable');

// Function to create a placeholder row
function createPlaceholderRow() {
  const row = document.createElement('tr');
  const columns = [
    '<div class="placeholder wide"></div>',
    '<div class="placeholder"></div>',
    '<div class="placeholder small"></div>',
    '<div class="placeholder small"></div>',
    '<div class="placeholder"></div>',
    '<div class="placeholder small"></div>',
    '<div class="placeholder wide"></div>',
    '<div class="placeholder"></div>',
    '<div class="placeholder small"></div>',
    '<div class="placeholder small"></div>',
    '<div class="placeholder"></div>',
    '<div class="placeholder small"></div>',
    '<div class="placeholder small"></div>',
    '<div class="placeholder small"></div>',
    '<div class="placeholder"></div>', '<div class="placeholder small"></div>',
    '<div class="placeholder small"></div>',
    '<div class="placeholder"></div>'
  ];

  columns.forEach(html => {
    const cell = document.createElement('td');
    cell.innerHTML = html;
    row.appendChild(cell);
  });
  return row;
}

// Remove the previous table body content
tableBody.innerHTML = '';

// Create a new table body (this part is actually redundant in most cases,
// as modifying innerHTML on the existing tableBody will achieve the same)
// const newTableBody = document.createElement('tbody');
// newTableBody.id = 'ordersManagmentTable'; // Ensure it has the correct ID

// Add the new rows to the (existing) table body
for (let i = 0; i < 7; i++) {
  tableBody.appendChild(createPlaceholderRow());
}

// If you were to create a *new* tbody element, you would need to replace the old one
// in the DOM. However, using innerHTML = '' and then appending to the existing
// tableBody is the more common and efficient approach.
// If you did create a new tbody:
// const table = tableBody.parentNode; // Get the parent table
// table.replaceChild(newTableBody, tableBody);
// tableBody = newTableBody; // Update the tableBody reference</script>

</body>
</html>
